{
  "name": "renamer",
  "description": "Rename files in bulk",
  "version": "1.1.1",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "bin": "bin/cli.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": "https://github.com/75lb/renamer",
  "files": [
    "index.js",
    "bin",
    "lib"
  ],
  "keywords": [
    "rename",
    "move",
    "mv",
    "file",
    "files",
    "bulk",
    "batch"
  ],
  "scripts": {
    "test": "test-runner test/*.js test/internals/*.js",
    "docs": "jsdoc2md index.js > docs/API.md",
    "cover": "nyc test-runner test/*.js && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "array-back": "^3.0.1",
    "chalk": "^2.4.2",
    "command-line-args": "^5.1.0",
    "command-line-plugin": "^0.1.2",
    "command-line-usage": "^5.0.5",
    "fast-diff": "^1.2.0",
    "glob": "^7.1.3",
    "global-modules": "^2.0.0",
    "load-module": "^1.0.0",
    "node-version-matches": "^1.0.1",
    "printj": "^1.2.1",
    "reduce-flatten": "^2.0.0",
    "reduce-unique": "^2.0.1",
    "stream-read-all": "^1.0.1",
    "typical": "^4.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "jsdoc-to-markdown": "^4.0.1",
    "mkdirp2": "^1.0.4",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "test-runner": "^0.6.0-14"
  }
}
